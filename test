local function createChatButton(name,cb,bg)
    local c = Instance.new("Frame")
    c.Name = name.."Container"
    c.Parent = ChatFrame
    c.BackgroundTransparency = 1
    c.Size = UDim2.new(1,-10,0,40)
    c.BorderSizePixel = 0
