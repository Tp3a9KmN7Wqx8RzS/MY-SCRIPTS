local function rSeq(o)
    local k = {}
    for i=0,1,0.1 do table.insert(k,ColorSequenceKeypoint.new(i,Color3.fromHSV((i+o)%1,1,1))) end
    return ColorSequence.new(k)
end
local off2 = 0
S.RunService.Heartbeat:Connect(function(dt)
    off2 = (off2 + dt*0.5) % 1
    grad2.Color = rSeq(off2)
end)

